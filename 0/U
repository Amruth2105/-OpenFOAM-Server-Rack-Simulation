FoamFile
{
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            noSlip;
    }
    
    outlet
    {
        type            noSlip;
    }
    
    floor
    {
        type            noSlip;
    }
    
    ceiling
    {
        type            noSlip;
    }
    
    frontAndBack
    {
        type            noSlip;
    }
}

