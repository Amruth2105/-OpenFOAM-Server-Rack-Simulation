FoamFile
{
    format      ascii;
    class       volScalarField;
    object      alphat;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    "(inlet|outlet|floor|ceiling|frontAndBack)"
    {
        type            compressible::alphatWallFunction;
        Prt             0.85;
        value           $internalField;
    }
}

